                                                                                                    queryPromise = proxy.model.dataSource.executeQuery(skipQuery);
                                                                                                    queryPromise.done(function (e) {
                                                                                                        dynamicItems(proxy, e.result);
                                                                                                    }).fail(function () {
                                                                                                        console.log("failure triggered")
                                                                                                    }).always(function (e) {
                                                                                                        proxy._removeLoadingClass();
                                                                                                    });
