        function checkEmail()
        {
            var email = document.getElementById("EmailEventEmail").value;
            if(email.length>0)
            {
                var pattern = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;

                if(email.toLowerCase().includes("jmsmucker"))
                {
                    return false;                                                           
                }

                return email.match(pattern);
            }           
            else{
                return false;
            }
        }

