@{
    ViewBag.Title = "Dispatch Response";
}

@model FarmerBrothers.Data.DispatchResponseModel

@if (!Model.IsERF)
{

<section class="container-fluid white-bg pad">
    <div class="row top-info">
        <div class="col-xs-12 col-sm-4 col-md-3">
            <b>Dispatch Response</b>
        </div>
    </div>
</section>

<section class="well">
    <div class="row top-info">
        <div class="customer-content margin-top">
            <div class="row customer clear-input">
                <div class="col-md-3 sm-pad-reset" style="width:50%;">
                    @if (Model.TechId != 0)
                    {
                        <div class="col-sm-6-o">
                            <span class="col-md-5">Tech ID:</span>
                            <div class="col-md-7">
                                <div class="form-group">
                                    @Model.TechId
                                </div>
                            </div>
                        </div>
                    }
                    <div class="col-sm-6-o">
                        <span class="col-md-5">Work Order ID:</span>
                        <div class="col-md-7">
                            <div class="form-group">
                                @Model.WorkOrderId
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6-o">
                        @Html.Raw(Model.Message);
                    </div>
                <div class="col-sm-6-o">
                </div>
                <div class="col-sm-6-o">
                </div>
            </div>
        </div>
    </div>
</div>
</section>

}
else
{
    <section class="container-fluid white-bg pad">
        <div class="row top-info">
            <div class="col-xs-12 col-sm-4 col-md-3">
                <b>ERF Update Response</b>
            </div>
        </div>
    </section>

    <section class="well">
        <div class="row top-info">
            <div class="customer-content margin-top">
                <div class="row customer clear-input">
                    <div class="col-md-3 sm-pad-reset">                        
                        
                        <div class="col-sm-6-o">
                            <text>
                                @Model.Message
                            </text>
                        </div>
                        <div class="col-sm-6-o">
                        </div>
                        <div class="col-sm-6-o">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

}